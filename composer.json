{
    "require": {
        "mpdf/mpdf": "v8.2.5",
        "dompdf/dompdf": "^3.1",
        "knplabs/knp-snappy": "^1.5",
        "tecnickcom/tc-lib-pdf": "^8.0",
        "gotenberg/gotenberg-php": "^2.13",
        "php-http/guzzle7-adapter": "^1.1",
        "spipu/html2pdf": "^5.3"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Podlodka\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Podlodka\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
